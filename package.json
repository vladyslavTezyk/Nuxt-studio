{
  "name": "nuxt-studio",
  "version": "0.0.1",
  "description": "Nuxt Studio for Nuxt Content",
  "repository": "@nuxtlabs/nuxt-studio",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/module/module.mjs",
    "./app": {
      "types": "./dist/app/main.d.ts",
      "default": "./dist/app/main.js"
    },
    "./app/utils": {
      "types": "./dist/app/utils.d.ts",
      "default": "./dist/app/utils.js"
    },
    "./app/service-worker": {
      "types": "./dist/app/service-worker.d.ts",
      "default": "./dist/app/service-worker.js"
    }
  },
  "main": "./dist/module/module.mjs",
  "scripts": {
    "prepack": "nuxt-module-build build src/module; vite build src/app",
    "dev": "npm run dev:prepare && STUDIO_DEV_SERVER=http://localhost:5151 nuxi dev playground/docus",
    "dev:minimal": "pnpm nuxt dev playground/minimal",
    "dev:app": "vite src/app --port 5151",
    "dev:build": "nuxi build playground/docus",
    "dev:prepare": "nuxt-module-build build --stub src/module && nuxt-module-build prepare src/module && nuxi prepare playground/docus",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "verify": "npm run lint && npm run typecheck && npm run test",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground/docus && vue-tsc --noEmit"
  },
  "devDependencies": {
    "@nuxt/content": "https://pkg.pr.new/@nuxt/content@22409fe",
    "@nuxt/eslint-config": "latest",
    "@nuxt/kit": "^4.1.2",
    "@nuxt/module-builder": "latest",
    "@nuxt/ui": "https://pkg.pr.new/@nuxt/ui@5111",
    "@octokit/types": "^15.0.0",
    "@tailwindcss/typography": "latest",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.36.0",
    "modern-monaco": "^0.2.2",
    "nuxt-studio": "workspace:*",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-libcss": "^1.1.2",
    "vitest": "^3.2.4",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.1.0",
    "zod": "^4.1.11"
  },
  "resolutions": {
    "@nuxt/content": "https://pkg.pr.new/@nuxt/content@22409fe",
    "@nuxtjs/mdc": "https://pkg.pr.new/@nuxtjs/mdc@61af819",
    "remark-mdc": "npm:remark-mdc-edge@3.6.0-29310522.024adeb"
  },
  "packageManager": "pnpm@10.17.1",
  "dependencies": {
    "@iconify-json/simple-icons": "^1.2.53",
    "@nuxtjs/mdc": "https://pkg.pr.new/@nuxtjs/mdc@61af819",
    "@nuxtlabs/monarch-mdc": "^0.8.1",
    "@unhead/vue": "^2.0.17",
    "@unpic/vue": "^1.0.0",
    "@vueuse/core": "^13.9.0",
    "defu": "^6.1.4",
    "destr": "^2.0.5",
    "idb-keyval": "^6.2.2",
    "minimark": "^0.2.0",
    "ofetch": "^1.4.1",
    "unstorage": "^1.17.1"
  }
}
